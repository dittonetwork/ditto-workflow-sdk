# Build-time environment variables for Vite (OPTIONAL)
# These are default values that can be overridden in the UI

# Default RPC URLs (can be changed in UI)
VITE_SEPOLIA_RPC_URL=https://rpc.ankr.com/eth_sepolia
VITE_BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Default Contract Addresses (can be changed in UI)
VITE_SEPOLIA_WORKFLOW_CONTRACT=
VITE_BASE_SEPOLIA_WORKFLOW_CONTRACT=

# Default IPFS Configuration (can be changed in UI)
VITE_IPFS_SERVICE_URL=https://api.ditto.network/ipfs
VITE_IPFS_GATEWAY=https://ipfs.io/ipfs

# Default API Configuration (can be changed in UI)
VITE_DITTO_API_URL=https://api.ditto.network

# Default Executor Address (can be changed in UI)
VITE_DEFAULT_EXECUTOR_ADDRESS=
